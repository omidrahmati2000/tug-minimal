# Production Docker Environment Variables
NODE_ENV=production
PORT=3001

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=your-secure-production-password
DB_NAME=myfuel

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-min-32-chars
JWT_EXPIRES_IN=24h

# pgAdmin Configuration
PGADMIN_EMAIL=admin@myfuel.com
PGADMIN_PASSWORD=secure-admin-password
PGADMIN_PORT=5050


# Fuel Station API Keys
FUEL_STATION_API_KEYS=prod_station_key_1,prod_station_key_2,prod_station_key_3,prod_station_key_4